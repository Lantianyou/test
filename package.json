    "@sanity/astro": "^3.0.0",
    "@sanity/client": "^6.15.13",
    "astro": "^4.6.0",
    "astro-portabletext": "^0.9.7",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.13",
    "tailwindcss": "^3.4.3",
    "tinacms": "^1.6.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@astrojs/react": "^3.3.0",
    "@astrojs/sitemap": "^3.1.2",
